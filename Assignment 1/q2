#include<iostream>
using namespace std;

int main()
{
    int arr[100];
    int n;
     cout<<"Enter no of elements:";
        cin>>n;
        cout<<"enter elements:";
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
        }
     //removing duplicates
     for(int i=0;i<n;i++)
     {
         for(int j=i+1;j<n;j++)
         {
             if(arr[i]==arr[j])
             {
                 for(int k=j;k<n-1;k++)
                 {
                     arr[k]=arr[k+1];
                 }
                 n--;
             }
             else
             {
                 j++;
             }
         }
     }
     //after removing duplicates
    cout<<"array after removing duplicates:"<<endl;
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
        }
    return 0;
}